        "build-prev": "esbuild entrypoint.js --sourcemap --tree-shaking=true --bundle --outdir=dist --target=esnext '--define:process={\"env\":\"production\",\"NODE_ENV\":\"production\"}' --define:global=window --analyze --format=cjs",
        "build-dart": "node ./build-for-dart.js",
        "build-python": "node ./build-for-python.js",
        "build-ts": "node ./build-for-ts.js",
        "build-ts-browser": "node ./build-for-ts-browser.js",
